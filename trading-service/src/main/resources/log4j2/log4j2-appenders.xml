<?xml version="1.0" encoding="UTF-8"?>
<Appenders>
    <Console name="Console" target="SYSTEM_OUT">
        <PatternLayout
                pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%traceId] [SPANID:%X{spanId}] %-5level [%t] %C{1.}:%L %msg%n%throwable"/>
    </Console>
    <Console name="AlarmConsole" target="SYSTEM_OUT">
        <PatternLayout
                pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [log---alarm] [%traceId] [SPANID:%X{spanId}] %-5level [%t] %C{1.}:%L %msg%n%throwable"/>
    </Console>
</Appenders>
